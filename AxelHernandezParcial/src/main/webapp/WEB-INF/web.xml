<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>AxelHernandezParcial</display-name>
    <servlet>
        <servlet-name>ServletExamen</servlet-name>
        <servlet-class>com.unitec.axelhernandezparcial.ServletExamen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletExamen.</servlet-name>
        <servlet-class>com.unitec.axelhernandezparcial.ServletExamen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeneradorCalificaciones</servlet-name>
        <servlet-class>com.unitec.axelhernandezparcial.GeneradorCalificaciones</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletExamen</servlet-name>
        <url-pattern>/ServletExamen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletExamen.</servlet-name>
        <url-pattern>/resultados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeneradorCalificaciones</servlet-name>
        <url-pattern>/GeneradorCalificaciones</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
